//////factorial///////
#include <stdio.h>

int main()
{
    int a, b = 1;
    printf("Enter the Value : ");
    scanf("%d",&a);
    
    for (int i = 1; i <= a; i++)
    {
        b = b * i;
    }
    printf("The factorial of %d is %d ",a,b);
}

///////fibonacci series///////
#include <stdio.h>
int main()
{
    int a = -1, b = 1, c, n;
    printf("Enter the Value : ");
    scanf("%d",&n);
  
    for (int i = 0; i < n; i++)
    {
        c = a + b;
        a = b;
        b = c;
        printf("%d\n",c);
    }
    
}
